    <flowDefinitions>
        <flows>
            <fullName>Sample_ergonomic_assessment</fullName>
            <label>[Uzorak] Ergonomska procjena</label>
            <screens>
                <fields>
                    <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;[Uzorak]&lt;span style=&quot;background-color: transparent; color: rgb(0, 0, 0); font-family: Arial; font-size: 11pt;&quot;&gt; &lt;/span&gt;&lt;a href=&quot;/sample/s/case/{!CreateCase}&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot;&gt;Za vas je kreiran {!GetCaseRecord.CaseNumber}&lt;/a&gt; slučaj. Stručnjak za ljudske potencijale će kontaktirati s vama. Idite na stavku &lt;a href=&quot;/sample/s/case/Case/Default&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot;&gt;Moji slučajevi&lt;/a&gt; kako biste provjerili trenutačni status.&lt;/p&gt;</fieldText>
                    <name>Confirmation_Message</name>
