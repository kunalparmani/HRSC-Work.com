    <flowDefinitions>
        <flows>
            <fullName>Sample_ergonomic_assessment</fullName>
            <label>[Príklad] Hodnotenie ergonómie</label>
            <screens>
                <fields>
                    <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;[Príklad]&lt;span style=&quot;background-color: transparent; color: rgb(0, 0, 0); font-family: Arial; font-size: 11pt;&quot;&gt; &lt;/span&gt;&lt;a href=&quot;/sample/s/case/{!CreateCase}&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot;&gt;Prípad {!GetCaseRecord.CaseNumber}&lt;/a&gt; bol pre vás vytvorený. HR špecialista vás bude kontaktovať. Prejdite na &lt;a href=&quot;/sample/s/case/Case/Default&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot;&gt;Moje prípady&lt;/a&gt; pre kontrolu aktuálneho stavu.&lt;/p&gt;</fieldText>
                    <name>Confirmation_Message</name>
