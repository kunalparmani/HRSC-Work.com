            <screens>
                <fields>
                    <fieldText>&lt;p&gt;Fant ingen aktive introduksjonsmaler!&lt;/p&gt;</fieldText>
                    <name>NoActiveOnBoardingTemplatetext</name>
                </fields>
                <name>Thank_You_Screen</name>
            </screens>
        </flows>
        <fullName>AssignOnboardingTasks</fullName>
        <label>Tildel introduksjonsoppgaver</label>
