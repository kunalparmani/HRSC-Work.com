    <flowDefinitions>
        <flows>
            <fullName>Sample_welcome_video</fullName>
            <label>Velkomstvideo</label>
            <screens>
                <fields>
                    <fieldText>&lt;p&gt;[Eksempel] Velkommen til teamet! Se denne videoen for å finne ut mer om oss.&amp;nbsp; &lt;/p&gt;</fieldText>
