                    <name>NoActiveOnBoardingTemplatetext</name>
                </fields>
                <name>Thank_You_Screen</name>
            </screens>
        </flows>
        <fullName>AssignOnboardingTasks</fullName>
        <label>Atribua tarefas de integração</label>
