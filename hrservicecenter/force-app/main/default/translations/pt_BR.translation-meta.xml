    <flowDefinitions>
        <flows>
            <fullName>Sample_welcome_video</fullName>
            <label>Vídeo de boas-vindas</label>
            <screens>
                <fields>
                    <fieldText>&lt;p&gt;[Amostra] Bem-vindo à nossa equipe! Assista a este vídeo para aprender sobre a nossa história.&amp;nbsp; &lt;/p&gt;</fieldText>
