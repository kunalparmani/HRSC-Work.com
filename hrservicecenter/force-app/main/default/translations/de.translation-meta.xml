                <fields>
                    <fieldText>&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;color: rgb(62, 62, 60); background-color: rgb(255, 255, 255);&quot;&gt;[Muster] &lt;/span&gt;&lt;a href=&quot;https://salesforce.com/&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot; style=&quot;background-color: rgb(255, 255, 255);&quot;&gt;Betriebsferien&lt;/a&gt;&lt;/p&gt;&lt;p&gt;[Muster] Planen Sie frühzeitig Ihren Urlaub.&lt;/p&gt;</fieldText>
                    <name>Body2</name>
                </fields>
                <fields>
                    <fieldText>Verstanden!</fieldText>
                    <validationRule>
                        <errorMessage>&lt;p&gt;Sie müssen zuerst zustimmen.&lt;/p&gt;</errorMessage>
                    </validationRule>
                    <name>Checkbox2</name>
                </fields>
                <fields>
                    <fieldText>&lt;p&gt;[Muster] Klicken Sie auf &quot;Senden&quot;. Anschließend erhalten Sie und Ihr Vorgesetzter eine E-Mail, in der bestätigt wird, dass Sie sich diese Informationen angesehen haben.&lt;/p&gt;</fieldText>
                    <name>Body3</name>
                </fields>
                <name>Review_Documents</name>
            </screens>
        </flows>
        <fullName>Review_Documents</fullName>
        <label>Dokumente überprüfen</label>
