    <flowDefinitions>
        <flows>
            <fullName>Sample_welcome_video</fullName>
            <label>Vídeo de bienvenida</label>
            <screens>
                <fields>
                    <fieldText>&lt;p&gt;[Muestra] ¡Le damos la bienvenida al equipo! Vea este vídeo para aprender sobre nuestra historia.&amp;nbsp; &lt;/p&gt;</fieldText>
